#include <stdio.h>

int main()
{
    double high;
    int n;
    scanf("%lf,%d", &high, &n);

    double distance=high;
    double heigh;

    for (int i = 1; i < n; i++)
    {
        high /= 2;
        distance += high*2;
    }

    heigh = high / 2;

    printf("%.1lf,%.1lf", distance, heigh);
    
    return 0;
}
